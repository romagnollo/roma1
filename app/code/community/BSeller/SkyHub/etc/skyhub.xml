            <brand>
                <code>brand</code>
                <label>Product Brand</label>
                <description>The products brand.</description>
                <cast_type>string</cast_type>
                <validation/>
                <required>0</required>
                <attribute_install_config>
                    <attribute_code>brand</attribute_code>
                    <label>Product Brand</label>
                    <backend_model/>
                    <type>int</type>
                    <input>select</input>
                    <source_model/>
                    <required>0</required>
                    <source>eav/entity_attribute_source_table</source>
                </attribute_install_config>
            </brand>
            <ean>
                <code>ean</code>
                <label>Product EAN</label>
                <description>The products valid EAN number.</description>
                <cast_type>string</cast_type>
                <validation/>
                <required>0</required>
                <attribute_install_config>
                    <attribute_code>ean</attribute_code>
                    <label>Product EAN</label>
                    <backend_model/>
                    <type>varchar</type>
