<config>
    <skyhub>
        <product_attributes>
            <sku>
                <code>sku</code>
                <label>Product Sku</label>
                <description>The products main SKU (Stock Keeping Unit).</description>
                <cast_type>string</cast_type>
                <attribute_code>sku</attribute_code>
                <validation/>
                <attribute_install_config>
                    <attribute_code>sku</attribute_code>
                    <label>SKU</label>
                    <backend_model>catalog/product_attribute_backend_sku</backend_model>
                    <type>static</type>
                    <input>text</input>
                    <source_model/>
                    <required>1</required>
                </attribute_install_config>
            </sku>
            <name>
                <code>name</code>
                <label>Product Name</label>
                <description>The products name.</description>
                <cast_type>string</cast_type>
                <validation/>
                <attribute_install_config>
                    <attribute_code>name</attribute_code>
                    <label>Product Name</label>
                    <backend_model/>
                    <type>varchar</type>
                    <input>text</input>
                    <source_model/>
                    <required>1</required>
                </attribute_install_config>
            </name>
            <description>
                <code>description</code>
                <label>Product Description</label>
                <description>The products description.</description>
                <cast_type>string</cast_type>
