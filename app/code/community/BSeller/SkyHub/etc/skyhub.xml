            <weight>
                <code>weight</code>
                <label>Product Weight</label>
                <description>The products weight (in kilograms always).</description>
                <cast_type>float</cast_type>
                <validation/>
                <required>1</required>
                <attribute_install_config>
                    <attribute_code>weight</attribute_code>
                    <label>Product Weight</label>
                    <backend_model/>
                    <type>decimal</type>
                    <input>text</input>
                    <source_model/>
                    <required>1</required>
                </attribute_install_config>
            </weight>
            <height>
                <code>height</code>
                <label>Product Height</label>
                <description>The products height (in centimeters always).</description>
                <cast_type>float</cast_type>
                <validation/>
                <required>1</required>
                <attribute_install_config>
                    <attribute_code>height</attribute_code>
