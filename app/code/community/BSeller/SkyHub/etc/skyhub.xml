                <code>width</code>
                <label>Product Width</label>
                <description>The products width (in centimeters always).</description>
                <cast_type>float</cast_type>
                <validation/>
                <required>1</required>
                <attribute_install_config>
                    <attribute_code>width</attribute_code>
                    <label>Product Width</label>
                    <backend_model/>
                    <type>decimal</type>
                    <input>text</input>
                    <source_model/>
                    <required>1</required>
                </attribute_install_config>
