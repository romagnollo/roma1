            <length>
                <code>length</code>
                <label>Product Length</label>
                <description>The products length (in centimeters always).</description>
                <cast_type>float</cast_type>
                <validation/>
                <required>1</required>
                <attribute_install_config>
                    <attribute_code>length</attribute_code>
                    <label>Product Length</label>
                    <backend_model/>
                    <type>decimal</type>
                    <input>text</input>
                    <source_model/>
                    <required>1</required>
                </attribute_install_config>
