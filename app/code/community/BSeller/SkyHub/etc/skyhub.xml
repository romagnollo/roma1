            <product>
                <attributes>
                    <blacklist>
                        <entity_id/>
                        <special_price/>
                        <attribute_set_id/>
                        <options_container/>
                        <media_gallery/>
                        <thumbnail/>
                        <small_image/>
                        <image/>
                        <msrp_display_actual_price_type/>
                        <msrp_enabled/>
                        <special_from_date/>
                        <special_to_date/>
                        <updated_at/>
                        <created_at/>
                        <visibility/>
                        <url_path/>
                        <url_key/>
                        <entity_type_id/>
