                    <input>select</input>
                    <source_model/>
                    <required>1</required>
                </attribute_install_config>
            </status>
            <qty>
                <code>qty</code>
                <label>Product Stock Quantity</label>
                <description>The products stock quantity available in store.</description>
                <cast_type>float</cast_type>
                <type>varchar</type>
                <input>text</input>
                <attribute_code/>
