                </attribute_install_config>
            </promotional_price>
            <cost>
                <code>cost</code>
                <label>Product Cost</label>
                <description>The products cost.</description>
                <cast_type>float</cast_type>
                <validation/>
                <required>0</required>
                <attribute_install_config>
                    <attribute_code>cost</attribute_code>
                    <label>Product Cost</label>
                    <backend_model/>
                    <type>decimal</type>
